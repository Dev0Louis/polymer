{
  "schemaVersion": 1,
  "id": "polymer-blocks",
  "version": "${version}",
  "name": "Polymer (Textured Blocks)",
  "description": "Polymer extension adding support for textured blocks!",
  "authors": [
    "Patbox"
  ],
  "contact": {
    "homepage": "https://pb4.eu",
    "sources": "https://github.com/Patbox/polymer"
  },
  "license": "LGPLv3",
  "icon": "assets/icon.png",
  "environment": "*",
  "entrypoints": {
    "polymc": [
      "eu.pb4.polymer.blocks.impl.PolyMcResourceEntrypoint"
    ]
  },
  "mixins": [
    "polymer-blocks.mixins.json"
  ],
  "depends": {
    "polymer-core": ">=${version}",
    "fabricloader": ">=0.14.0"
  },
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ],
      "parent": "polymer-bundled",
      "update-checker": false
    }
  }
}
