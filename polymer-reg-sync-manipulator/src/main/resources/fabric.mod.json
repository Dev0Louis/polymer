{
  "schemaVersion": 1,
  "id": "polymer-registry-sync-manipulator",
  "version": "${version}",
  "name": "Polymer Registry Sync Manipulator",
  "description": "Library for removing entries from registry sync!",
  "authors": [
    "Patbox"
  ],
  "contact": {
    "homepage": "https://pb4.eu",
    "sources": "https://github.com/Patbox/polymer",
    "issues": "https://github.com/Patbox/polymer/issues"
  },

  "license": "MIT",
  "icon": "assets/icon.png",
  "environment": "*",
  "entrypoints": {
    "preLaunch": ["eu.pb4.polymer.rsm.impl.InitCheck::check"]
  },
  "mixins": [
    "polymer-rsm.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.0"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ],
      "discord": "https://pb4.eu/discord"
    }
  }
}
